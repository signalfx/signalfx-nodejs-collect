{
  "name": "signalfx-collect",
  "version": "2.0.0-beta3",
  "description": "SignalFx library to collect nodejs metrics and report to SignalFx",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "mocha",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "gcstats",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://signalfx-collect.s3.amazonaws.com"
  },
  "homepage": "https://signalfx.com",
  "author": {
    "name": "SignalFx, Inc",
    "email": "info@signalfx.com",
    "url": "https://signalfx.com"
  },
  "license": "Apache Software License v2",
  "dependencies": {
    "event-loop-stats": "^1.2.0",
    "fqdn": "0.0.3",
    "node-memwatch-sfx": "1.0.0-sfx1",
    "signalfx": "^6.0.1",
    "@mapbox/node-pre-gyp": "^1.0.5"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signalfx/signalfx-nodejs-collect"
  }
}
